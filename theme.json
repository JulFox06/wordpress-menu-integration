{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "customTemplates": [],
  "settings": {
    "appearanceTools": true,
    "blocks": {},
    "border": {},
    "color": {
      "background": true,
      "custom": true,
      "customDuotone": true,
      "customGradient": true,
      "defaultDuotone": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "duotone": [],
      "gradients": [],
      "link": true,
      "palette": [
        {
          "color": "#000000",
          "name": "Black",
          "slug": "black"
        },
        {
          "color": "#FFFFFF",
          "name": "White",
          "slug": "white"
        },
        {
          "color": "transparent",
          "name": "Transparent",
          "slug": "transparent"
        }
      ],
      "text": true
    },
    "custom": {},
    "dimensions": {
      "minHeight": true
    },
    "layout": {
      "contentSize": "768px",
      "wideSize": "1200px"
    },
    "position": {
      "sticky": false
    },
    "shadow": {
      "defaultPresets": false
    },
    "spacing": {
      "blockGap": true,
      "customSpacingSize": true,
      "margin": true,
      "padding": true,
      "spacingScale": {
        "operator": "*",
        "increment": 1.5,
        "steps": 7,
        "mediumStep": 1.5,
        "unit": "rem"
      },
      "spacingSizes": [],
      "units": [ "px", "%", "em", "rem", "vh", "vw" ]
    },
    "typography": {
      "customFontSize": true,
      "dropCap": true,
      "fontStyle": true,
      "fontWeight": true,
      "letterSpacing": true,
      "lineHeight": false,
      "textColumns": false,
      "textDecoration": true,
      "textTransform": true,
      "writingMode": false,
      "fontFamilies": [
        {
          "name": "System UI",
          "slug": "default",
          "fontFamily": "system-ui, sans-serif"
        }
      ],
      "fluid": false,
      "fontSizes": [
        {
          "name": "Taille 1",
          "size": "var(--heading-1-font-size, 1.5rem)",
          "slug": "heading-1"
        },
        {
          "name": "Taille 2",
          "size": "var(--heading-2-font-size, 2.25rem)",
          "slug": "heading-2"
        },
        {
          "name": "Taille 3",
          "size": "var(--heading-3-font-size, 2rem)",
          "slug": "heading-3"
        },
        {
          "name": "Taille 4",
          "size": "var(--heading-4-font-size, 1.75rem)",
          "slug": "heading-4"
        },
        {
          "name": "Taille 5",
          "size": "var(--heading-5-font-size, 1.5rem)",
          "slug": "heading-5"
        },
        {
          "name": "Taille 6",
          "size": "var(--heading-6-font-size, 1.25rem)",
          "slug": "heading-6"
        },
        {
          "name": "Paragraph",
          "size": "var(--paragraph-font-size, 1rem)",
          "slug": "paragraph"
        }
      ]
    },
    "useRootPaddingAwareAlignments": true
  },
  "styles": {
    "blocks": {
      "core/heading": {
        "typography": {
          "fontFamily": "var:preset|font-family|default"
        }
      }
    },
    "border": {},
    "color": {},
    "dimensions": {},
    "elements": {},
    "filter": {},
    "shadow": {},
    "spacing": {},
    "typography": {
      "fontFamily": "var:preset|font-family|default",
      "fontSize": "var:preset|font-size|paragraph",
      "fontStyle": "normal",
      "fontWeight": "400",
      "letterSpacing": "var(--theme-letter-spacing, 0)",
      "lineHeight": "var(--theme-line-height, 1.5)"
    }
  }
}
